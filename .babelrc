{
  "presets": ["env", "react"],
  "plugins": [
    "transform-class-properties",
    "react-hot-loader/babel",
    "transform-object-rest-spread"
  ],
  "env": {
    "production": {
      "plugins": [
        [
          "react-remove-properties",
          {
            "properties": ["data-test-id"]
          }
        ]
      ]
    }
  }
}
